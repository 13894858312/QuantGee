<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.CurrentAll" table="CurrentAll" schema="quantgee_data">
        <property name="stockID" column="code"/>
        <property name="stockName" column="name"/>
        <property name="changepercent" column="changepercent"/>
        <property name="trade" column="trade"/>
        <property name="open" column="open"/>
        <property name="high" column="high"/>
        <property name="low" column="low"/>
        <property name="settlement" column="settlement"/>
        <property name="volume" column="volume"/>
        <property name="turnoverratio" column="turnoverratio"/>
        <property name="amount" column="amount"/>
        <property name="per" column="per"/>
        <property name="pb" column="pb"/>
        <property name="mktcap" column="mktcap"/>
        <property name="nmc" column="nmc"/>
        <property name="index" column="index"/>
    </class>
</hibernate-mapping>