<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.HistStock" table="HistStock" schema="quantgee_data">
        <property name="date" column="date"/>
        <property name="open" column="open"/>
        <property name="high" column="high"/>
        <property name="close" column="close"/>
        <property name="low" column="low"/>
        <property name="volume" column="volume"/>
        <property name="amount" column="amount"/>
        <property name="stockId" column="stockID"/>
    </class>
</hibernate-mapping>