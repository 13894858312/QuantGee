<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.CollectStrategy" table="CollectStrategy" schema="quantgee_data">
        <composite-id mapped="true" class="bean.CollectStrategyPK">
            <key-property name="userId">
                <column name="userID" sql-type="varchar(20)" length="20"/>
            </key-property>
            <key-property name="strategyId">
                <column name="strategyID" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>