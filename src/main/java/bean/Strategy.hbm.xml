<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.Strategy" table="Strategy" schema="quantgee_data">
        <id name="strategyId">
            <column name="strategyID" sql-type="int(11)"/>
        </id>
        <property name="strategyName">
            <column name="strategyName" sql-type="text"/>
        </property>
        <property name="strategyType">
            <column name="strategyType" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="strategyContent">
            <column name="strategyContent" sql-type="text" not-null="true"/>
        </property>
        <property name="userId">
            <column name="userID" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="posted">
            <column name="posted" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>