<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.Strategy" table="Strategy" schema="quant_gee">
        <id name="strategyId" column="strategyID"/>
        <property name="strategyName" column="strategyName"/>
        <property name="strategyType" column="strategyType"/>
        <property name="strategyContent" column="strategyContent"/>
    </class>
</hibernate-mapping>