<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.HoldingStock" table="HoldingStock" schema="quantgee_data">
        <composite-id mapped="true" class="bean.HoldingStockPK">
            <key-property name="userId" column="userID"/>
            <key-property name="stockId" column="stockID"/>
        </composite-id>
        <property name="holdNum" column="holdNum"/>
    </class>
</hibernate-mapping>