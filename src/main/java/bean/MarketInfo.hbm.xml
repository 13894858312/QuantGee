<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.MarketInfo" table="MarketInfo" schema="quantgee_data">
        <id name="stockID" column="code"/>
        <property name="stockName" column="name"/>
        <property name="market" column="c_name"/>
    </class>
</hibernate-mapping>