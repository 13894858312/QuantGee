<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.Account" table="Account" schema="quant_gee">
        <id name="accountId">
            <column name="accountId" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="password">
            <column name="password" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="registerDate">
            <column name="registerDate" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="identity">
            <column name="identity" sql-type="int(11)"/>
        </property>
    </class>
    <class name="bean.Analysis" table="Analysis" schema="quant_gee">
        <id name="date">
            <column name="date" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="num">
            <column name="num" sql-type="int(11)"/>
        </property>
    </class>
    <class name="bean.CollectStock" table="CollectStock" schema="quant_gee">
        <id name="userId">
            <column name="userID" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="stockId">
            <column name="stockID" sql-type="varchar(6)" length="6"/>
        </property>
    </class>
    <class name="bean.CollectStrategy" table="CollectStrategy" schema="quant_gee">
        <id name="userId">
            <column name="userID" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="strategyId">
            <column name="strategyID" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
    <class name="bean.MarketInfo" table="MarketInfo" schema="quant_gee">
        <id name="stockId">
            <column name="stockID" sql-type="varchar(6)" length="6"/>
        </id>
        <property name="srockName">
            <column name="srockName" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="stockMarket">
            <column name="stockMarket" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
    </class>
    <class name="bean.Stock" table="Stock" schema="quant_gee">
        <composite-id mapped="true" class="bean.StockPK">
            <key-property name="stockCode">
                <column name="stockCode" sql-type="varchar(6)" length="6"/>
            </key-property>
            <key-property name="date">
                <column name="date" sql-type="varchar(10)" length="10"/>
            </key-property>
        </composite-id>
        <property name="open">
            <column name="open" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="high">
            <column name="high" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="close">
            <column name="close" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="low">
            <column name="low" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="volume">
            <column name="volume" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="priceChange">
            <column name="price_change" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="pChange">
            <column name="p_change" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="ma5">
            <column name="ma5" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="ma10">
            <column name="ma10" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="ma20">
            <column name="ma20" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="vMa5">
            <column name="v_ma5" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="vMa10">
            <column name="v_ma10" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="vMa20">
            <column name="v_ma20" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="turnover">
            <column name="turnover" sql-type="double" precision="-1" not-null="true"/>
        </property>
    </class>
    <class name="bean.Strategy" table="Strategy" schema="quant_gee">
        <id name="strategyId">
            <column name="strategyID" sql-type="int(11)"/>
        </id>
        <property name="strategyName">
            <column name="strategyName" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="strategyType">
            <column name="strategyType" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="strategyContent">
            <column name="strategyContent" sql-type="varchar(10)" length="10"/>
        </property>
    </class>
    <class name="bean.Trade" table="Trade" schema="quant_gee">
        <id name="tradeId">
            <column name="tradeID" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="userID" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="time">
            <column name="time" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="stockId">
            <column name="stockID" sql-type="varchar(6)" length="6"/>
        </property>
        <property name="action">
            <column name="action" sql-type="int(11)"/>
        </property>
        <property name="numOfStock">
            <column name="numOfStock" sql-type="int(11)"/>
        </property>
    </class>
    <class name="bean.TradeUser" table="TradeUser" schema="quant_gee">
        <composite-id mapped="true" class="bean.TradeUserPK">
            <key-property name="userId">
                <column name="userID" sql-type="varchar(20)" length="20"/>
            </key-property>
            <key-property name="stockId">
                <column name="stockID" sql-type="varchar(6)" length="6"/>
            </key-property>
        </composite-id>
        <property name="currentHoldingNum">
            <column name="currentHoldingNum" sql-type="int(11)"/>
        </property>
        <property name="currentYield">
            <column name="currentYield" sql-type="double" precision="-1"/>
        </property>
    </class>
    <class name="bean.User" table="User" schema="quant_gee">
        <id name="userId">
            <column name="userID" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="alterName">
            <column name="alterName" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="phoneNumber">
            <column name="phoneNumber" sql-type="varchar(11)" length="11" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>