<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.History" table="History" schema="quantgee_data">
        <id name="index">
            <column name="index" sql-type="int(11)"/>
        </id>
        <property name="time">
            <column name="time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="open">
            <column name="open" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="high">
            <column name="high" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="close">
            <column name="close" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="low">
            <column name="low" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="volume">
            <column name="volume" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="amount">
            <column name="amount" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="stockId">
            <column name="stockID" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>