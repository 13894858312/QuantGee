<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.Stock" table="Stock" schema="quantgee_data">
        <id name="date" column="date"/>
        <property name="volume" column="volume"/>
        <property name="open" column="open"/>
        <property name="high" column="high"/>
        <property name="close" column="close"/>
        <property name="low" column="low"/>
        <property name="priceChange" column="price_change"/>
        <property name="pChange" column="p_change"/>
        <property name="ma5" column="ma5"/>
        <property name="ma10" column="ma10"/>
        <property name="ma20" column="ma20"/>
        <property name="vMa5" column="v_ma5"/>
        <property name="vMa10" column="v_ma10"/>
        <property name="vMa20" column="v_ma20"/>
        <property name="stockId" column="stockID"/>
    </class>
</hibernate-mapping>