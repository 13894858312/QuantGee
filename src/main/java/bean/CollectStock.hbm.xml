<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.CollectStock" table="CollectStock" schema="quantgee_data">
        <id name="index">
            <column name="index" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="userID" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="stockId">
            <column name="stockID" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>