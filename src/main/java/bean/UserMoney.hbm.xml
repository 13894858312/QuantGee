<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.UserMoney" table="UserMoney" schema="quantgee_data">
        <id name="userId">
            <column name="userID" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="remainMoney">
            <column name="remainMoney" sql-type="double" precision="-1"/>
        </property>
    </class>
</hibernate-mapping>