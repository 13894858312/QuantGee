<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.News" table="News" schema="quantgee_data">
        <id name="index">
            <column name="index" sql-type="bigint(20)" not-null="true"/>
        </id>
        <property name="classify">
            <column name="classify" sql-type="text" not-null="true"/>
        </property>
        <property name="title">
            <column name="title" sql-type="text" not-null="true"/>
        </property>
        <property name="time">
            <column name="time" sql-type="text" not-null="true"/>
        </property>
        <property name="url">
            <column name="url" sql-type="text" not-null="true"/>
        </property>
        <property name="content">
            <column name="content" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>