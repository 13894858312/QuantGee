<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.Boll" table="BOLL" schema="quantgee_data">
        <id name="index">
            <column name="index" sql-type="int(11)"/>
        </id>
        <property name="code">
            <column name="code" sql-type="varchar(6)" length="6" not-null="true"/>
        </property>
        <property name="date">
            <column name="date" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="mid">
            <column name="mid" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="up">
            <column name="up" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="low">
            <column name="low" sql-type="double" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>